        <!-- Completed Intervention -->
        <record id="demo_intervention_1" model="patrimoine.intervention">
            <field name="name">Remplacement disque dur PC002</field>
            <field name="equipment_id" ref="demo_equipment_pc_2"/>
            <field name="intervention_type">repair</field>
            <field name="priority">high</field>
            <field name="requester_id" ref="demo_partner_manager"/>
            <field name="technician_id" ref="demo_partner_technician_1"/>
            <field name="location_id" ref="location_ensah_lab_info"/>
            <field name="request_date">2024-11-25 09:00:00</field>
            <field name="intervention_date">2024-11-27</field>
            <field name="start_datetime">2024-11-27 09:00:00</field>
            <field name="end_datetime">2024-11-27 11:00:00</field>
            <field name="state">done</field>
            <field name="description">&lt;p&gt;Le disque dur de l'ordinateur PC002 est défaillant. L'ordinateur ne démarre plus et affiche un message d'erreur "No bootable device".&lt;/p&gt;</field>
            <field name="diagnostic">&lt;p&gt;Après diagnostic, le disque dur SSD est HS (défaillance mécanique). Besoin de remplacement.&lt;/p&gt;</field>
            <field name="work_done">&lt;p&gt;&lt;strong&gt;Travaux effectués:&lt;/strong&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;Retrait de l'ancien SSD défaillant&lt;/li&gt;&lt;li&gt;Installation du nouveau SSD 512GB&lt;/li&gt;&lt;li&gt;Installation système Windows 10 Pro&lt;/li&gt;&lt;li&gt;Installation logiciels de base&lt;/li&gt;&lt;li&gt;Tests de fonctionnement&lt;/li&gt;&lt;/ul&gt;</field>
            <field name="recommendations">&lt;p&gt;Recommandation: Effectuer des sauvegardes régulières. Prévoir un remplacement préventif des SSD après 3-4 ans d'utilisation intensive.&lt;/p&gt;</field>
            <field name="labor_cost">800.00</field>
            <field name="other_costs">200.00</field>
            <field name="resolution_status">fixed</field>
            <field name="customer_satisfaction">very_satisfied</field>
        </record>

